<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="UserAccessView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  SelectList="UserAccess.ID, 
       UserAccess.USER_NAME, 
       UserAccess.PASSWORD, 
       UserAccess.FULL_NAME, 
       UserAccess.DESCR, 
       UserAccess.TITLE, 
       UserAccess.CONTACT_NO, 
       UserAccess.COMPANY_ID, 
       UserAccess.USER_TYPE, 
       (CASE WHEN UserAccess.ACTIVE_PERIOD_FROM > SYSDATE OR UserAccess.ACTIVE_PERIOD_TO &lt; SYSDATE THEN 'D' ELSE 'A' END) AS ACCESS_STATUS, 
       UserAccess.USER_INITIAL, 
       UserAccess.USER_DIVISION, 
       (SELECT UCP.USER_CUSTOMER FROM FCS_USER_CUST_PRIV UCP WHERE UCP.USER_NAME = UserAccess.USER_NAME) AS USER_CUSTOMER, 
       (SELECT LC.VALUE FROM LOOKUP_CODE LC WHERE LC.TITLE = 'APP_REPORT_DOMAIN') AS REPORT_DOMAIN"
  FromList="APP_USER_ACCESS UserAccess"
  Where="UPPER(UserAccess.USER_NAME) = UPPER(:userLogin)
AND UPPER(UserAccess.PASSWORD) = UPPER(:userPassword)"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false">
  <Variable
    Name="userLogin"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="userPassword"
    Kind="where"
    Type="java.lang.String"/>
  <EntityUsage
    Name="UserAccess"
    Entity="app.fpp.model.entities.useraccessmenu.UserAccess"/>
  <ViewAttribute
    Name="Id"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="UserAccess"
    AliasName="ID"/>
  <ViewAttribute
    Name="UserName"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserName"
    EntityUsage="UserAccess"
    AliasName="USER_NAME"/>
  <ViewAttribute
    Name="Password"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Password"
    EntityUsage="UserAccess"
    AliasName="PASSWORD"/>
  <ViewAttribute
    Name="FullName"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="FullName"
    EntityUsage="UserAccess"
    AliasName="FULL_NAME"/>
  <ViewAttribute
    Name="Descr"
    PrecisionRule="true"
    EntityAttrName="Descr"
    EntityUsage="UserAccess"
    AliasName="DESCR"/>
  <ViewAttribute
    Name="Title"
    PrecisionRule="true"
    EntityAttrName="Title"
    EntityUsage="UserAccess"
    AliasName="TITLE"/>
  <ViewAttribute
    Name="ContactNo"
    PrecisionRule="true"
    EntityAttrName="ContactNo"
    EntityUsage="UserAccess"
    AliasName="CONTACT_NO"/>
  <ViewAttribute
    Name="CompanyId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="CompanyId"
    EntityUsage="UserAccess"
    AliasName="COMPANY_ID"/>
  <ViewAttribute
    Name="UserType"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserType"
    EntityUsage="UserAccess"
    AliasName="USER_TYPE"/>
  <ViewAttribute
    Name="AccessStatus"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ACCESS_STATUS"
    SQLType="VARCHAR"
    Precision="255"
    Expression="(CASE WHEN UserAccess.ACTIVE_PERIOD_FROM > SYSDATE OR UserAccess.ACTIVE_PERIOD_TO &lt; SYSDATE THEN 'D' ELSE 'A' END)"/>
  <ViewAttribute
    Name="UserInitial"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserInitial"
    EntityUsage="UserAccess"
    AliasName="USER_INITIAL"/>
  <ViewAttribute
    Name="UserDivision"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserDivision"
    EntityUsage="UserAccess"
    AliasName="USER_DIVISION"/>
  <ViewAttribute
    Name="UserCustomer"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="USER_CUSTOMER"
    SQLType="VARCHAR"
    Precision="255"
    Expression="SELECT UCP.USER_CUSTOMER FROM FCS_USER_CUST_PRIV UCP WHERE UCP.USER_NAME = UserAccess.USER_NAME"/>
  <ViewAttribute
    Name="ReportDomain"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="REPORT_DOMAIN"
    SQLType="VARCHAR"
    Precision="255"
    Expression="SELECT LC.VALUE FROM LOOKUP_CODE LC WHERE LC.TITLE = 'APP_REPORT_DOMAIN'"/>
</ViewObject>

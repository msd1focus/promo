    Precision="255"
    Expression="SELECT ARC.ATTRIBUTE3 FROM APPS.AR_CUSTOMERS ARC WHERE ARC.ATTRIBUTE4 = ProdRegionArea.AREA_CODE GROUP BY ARC.ATTRIBUTE3"/>

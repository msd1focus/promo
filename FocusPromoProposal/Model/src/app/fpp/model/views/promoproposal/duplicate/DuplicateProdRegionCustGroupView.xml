  <SQLQuery>
    <![CDATA[SELECT ProdRegionCustGroup.PROMO_PRODUK_ID, 
       ProdRegionCustGroup.CUST_GROUP, 
       ProdRegionCustGroup.NOTES, 
       (SELECT DISTINCT LISTAGG(AC.ATTRIBUTE3,'|') WITHIN GROUP (ORDER BY PRCG.PROMO_PRODUK_ID,          PRCG.CUST_GROUP, PRCG.NOTES) OVER (PARTITION BY PRCG.PROMO_PRODUK_ID,            PRCG.CUST_GROUP, PRCG.NOTES)  AS REGION_CODE FROM PROD_REGION_CUST_GROUP PRCG, APPS.AR_CUSTOMERS AC, APP_USER_REGION AUR WHERE AC.ATTRIBUTE8 = PRCG.CUST_GROUP AND AC.ATTRIBUTE3 = AUR.REGION_CODE AND PRCG.PROMO_PRODUK_ID = ProdRegionCustGroup.PROMO_PRODUK_ID AND PRCG.CUST_GROUP = ProdRegionCustGroup.CUST_GROUP GROUP BY PRCG.PROMO_PRODUK_ID,  PRCG.NOTES,         PRCG.CUST_GROUP,  AC.ATTRIBUTE3) AS REGION_CODE, 
       (ProdRegionCustGroup.CUST_GROUP) AS CUST_GROUP_NAME
FROM PROD_REGION_CUST_GROUP ProdRegionCustGroup
WHERE ProdRegionCustGroup.PROMO_PRODUK_ID = :promoProdukId]]>
  </SQLQuery>

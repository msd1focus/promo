<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="RunNumberView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false"
  SelectList="RunNumber.RUN_NUM_TYPE, 
       RunNumber.USER_NAME, 
       AppUserAccess.USER_INITIAL, 
       RunNumber.RUN_YEAR, 
       RunNumber.RUN_MONTH, 
       RunNumber.VALUE, 
       TO_CHAR(TO_DATE(RunNumber.RUN_YEAR, 'YY'), 'YYYY') AS YEAR_STR, 
       TO_CHAR(TO_DATE(RunNumber.RUN_MONTH, 'MM'), 'MONTH') AS RUN_MONTH_STR, 
       AppUserAccess.ID"
  FromList="RUN_NUMBER RunNumber, APP_USER_ACCESS AppUserAccess"
  OrderBy="RunNumber.RUN_YEAR,RunNumber.RUN_MONTH DESC"
  Where="RunNumber.USER_NAME = AppUserAccess.USER_NAME">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="app.fpp.model.views.masterdata.RunNumberView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <ViewAccessor
    Name="LovRunNumType"
    ViewObjectName="app.fpp.model.views.masterdata.lov.LookupCodeView">
    <ParameterMap>
      <PIMap Variable="luTitle">
        <TransientExpression><![CDATA['APP_RUN_NUM_TYPE']]></TransientExpression>
      </PIMap>
    </ParameterMap>
  </ViewAccessor>
  <ViewAccessor
    Name="LovCompType"
    ViewObjectName="app.fpp.model.views.masterdata.lov.LookupCodeView">
    <ParameterMap>
      <PIMap Variable="luTitle">
        <TransientExpression><![CDATA['APP_COMP_TYPE']]></TransientExpression>
      </PIMap>
    </ParameterMap>
  </ViewAccessor>
  <ListBinding
    Name="LOV_RunNumType"
    ListVOName="LovRunNumType"
    ListRangeSize="-1"
    NullValueFlag="none"
    NullValueId="LOV_RunNumType_LOVUIHints_NullValueId"
    MRUCount="0">
    <AttrArray Name="AttrNames">
      <Item Value="RunNumType"/>
    </AttrArray>
    <AttrArray Name="ListAttrNames">
      <Item Value="Value"/>
    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="Descr"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <EntityUsage
    Name="RunNumber"
    Entity="app.fpp.model.entities.masterdata.RunNumber"
    ReadOnly="true"
    DeleteParticipant="false"/>
  <EntityUsage
    Name="AppUserAccess"
    Entity="app.fpp.model.entities.useraccess.AppUserAccess"
    SourceUsage="app.fpp.model.views.masterdata.RunNumberView.RunNumber"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <ViewAttribute
    Name="RunNumType"
    IsUpdateable="false"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="RunNumType"
    EntityUsage="RunNumber"
    AliasName="RUN_NUM_TYPE"
    LOVName="LOV_RunNumType">
    <Properties>
      <SchemaBasedProperties>
        <CONTROLTYPE
          Value="choice"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="UserName"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserName"
    EntityUsage="RunNumber"
    AliasName="USER_NAME"/>
  <ViewAttribute
    Name="UserInitial"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserInitial"
    EntityUsage="AppUserAccess"
    AliasName="USER_INITIAL"/>
  <ViewAttribute
    Name="RunYear"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="RunYear"
    EntityUsage="RunNumber"
    AliasName="RUN_YEAR"/>
  <ViewAttribute
    Name="RunMonth"
    PrecisionRule="true"
    EntityAttrName="RunMonth"
    EntityUsage="RunNumber"
    AliasName="RUN_MONTH"/>
  <ViewAttribute
    Name="Value"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Value"
    EntityUsage="RunNumber"
    AliasName="VALUE"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="app.fpp.model.ModelBundle"/>
  </ResourceBundle>
  <ViewAttribute
    Name="YearStr"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="YEAR_STR"
    SQLType="VARCHAR"
    Precision="255"
    Expression="TO_CHAR(TO_DATE(RunNumber.RUN_YEAR, 'YY'), 'YYYY')">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="YEAR"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="RunMonthStr"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="RUN_MONTH_STR"
    SQLType="VARCHAR"
    Precision="255"
    Expression="TO_CHAR(TO_DATE(RunNumber.RUN_MONTH, 'MM'), 'MONTH')">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="MONTH"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Id"
    IsUpdateable="while_insert"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="AppUserAccess"
    AliasName="ID"/>
</ViewObject>

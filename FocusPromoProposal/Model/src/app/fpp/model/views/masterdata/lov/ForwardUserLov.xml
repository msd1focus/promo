<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ForwardUserLov"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  OrderBy='"FULL_NAME"'
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="app.fpp.model.views.masterdata.lov.ForwardUserLov_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="userName"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[select ua.user_name, ua.full_name from app_user_access ua, app_user_access_roles uar, app_roles ar
where ua.user_name = uar.user_name
and ar.name = uar.role
and ar.forward_flg = 'Y'
and ua.user_name <> :userName]]>
  </SQLQuery>
  <ViewAttribute
    Name="UserName"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="USER_NAME"
    Expression="USER_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="FullName"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="90"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="FULL_NAME"
    Expression="FULL_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="90"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="UserName"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="app.fpp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

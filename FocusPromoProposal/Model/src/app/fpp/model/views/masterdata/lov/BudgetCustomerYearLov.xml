<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="BudgetCustomerYearLov"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  OrderBy='"BUDGET_YEAR"'
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="app.fpp.model.views.masterdata.lov.BudgetCustomerYearLov_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[SELECT DISTINCT(BUDGET_YEAR) AS BUDGET_YEAR FROM (
SELECT BudgetCustHdr.BUDGET_YEAR AS BUDGET_YEAR FROM BUDGET_CUST_HDR BudgetCustHdr WHERE TO_DATE(BudgetCustHdr.BUDGET_YEAR,'YYYY') < SYSDATE
UNION ALL
SELECT TO_CHAR(TO_NUMBER(TO_CHAR(SYSDATE, 'YYYY'))) AS BUDGET_YEAR FROM DUAL
UNION ALL
SELECT TO_CHAR(TO_NUMBER(TO_CHAR(ADD_MONTHS(SYSDATE, 12)-1, 'YYYY'))) AS BUDGET_YEAR FROM DUAL
)]]>
  </SQLQuery>
  <ViewAttribute
    Name="BudgetYear"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="4"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="BUDGET_YEAR"
    Expression="BUDGET_YEAR"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="BudgetYear"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="app.fpp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

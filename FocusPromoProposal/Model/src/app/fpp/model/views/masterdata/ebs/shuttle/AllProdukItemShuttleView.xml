<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="AllProdukItemShuttleView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  OrderBy='"ITEM_DESCRIPTION"'
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="app.fpp.model.views.masterdata.ebs.shuttle.AllProdukItemShuttleView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="setCategory"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="setClass"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="setBrand"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="setExt"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="setPack"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery>
    <![CDATA[SELECT FcsViewItemMasterCategory.ITEM, 
       NVL(FcsViewItemMasterCategory.ITEM_DESCRIPTION, FcsViewItemMasterCategory.ITEM) AS ITEM_DESCRIPTION, 
       FcsViewItemMasterCategory.SET_PACKAGING
FROM APPS.FCS_VIEW_ITEM_MASTER_CATEGORY FcsViewItemMasterCategory
WHERE FcsViewItemMasterCategory.SET_CATEGORY = :setCategory
AND FcsViewItemMasterCategory.SET_CLASS = :setClass
AND FcsViewItemMasterCategory.SET_BRAND = :setBrand
AND FcsViewItemMasterCategory.SET_EXT = :setExt
AND FcsViewItemMasterCategory.SET_PACKAGING = :setPack
AND FcsViewItemMasterCategory.TYPE_TABLE = 'COMBINATION'
AND FcsViewItemMasterCategory.ITEM_ID IS NOT NULL
GROUP BY FcsViewItemMasterCategory.ITEM, FcsViewItemMasterCategory.ITEM_DESCRIPTION, FcsViewItemMasterCategory.SET_PACKAGING]]>
  </SQLQuery>
  <ViewAttribute
    Name="Item"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITEM"
    Expression="ITEM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ItemDescription"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="240"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITEM_DESCRIPTION"
    Expression="ITEM_DESCRIPTION"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="240"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SetPackaging"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="40"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SET_PACKAGING"
    Expression="SET_PACKAGING"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="Item"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="app.fpp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LookupCodeView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  SelectList="LookupCode.LOOKUP_ID, 
       LookupCode.TITLE, 
       LookupCode.VALUE, 
       LookupCode.DESCR"
  FromList="LOOKUP_CODE LookupCode"
  Where="LookupCode.TITLE = :luTitle"
  OrderBy="LookupCode.DESCR"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|VarAccess"/>
  </DesignTime>
  <Variable
    Name="luTitle"
    Kind="where"
    Type="java.lang.String"/>
  <EntityUsage
    Name="LookupCode"
    Entity="app.fpp.model.entities.masterdata.LookupCode"/>
  <ViewAttribute
    Name="LookupId"
    IsUpdateable="while_insert"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="LookupId"
    EntityUsage="LookupCode"
    AliasName="LOOKUP_ID"/>
  <ViewAttribute
    Name="Title"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Title"
    EntityUsage="LookupCode"
    AliasName="TITLE"/>
  <ViewAttribute
    Name="Value"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Value"
    EntityUsage="LookupCode"
    AliasName="VALUE"/>
  <ViewAttribute
    Name="Descr"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Descr"
    EntityUsage="LookupCode"
    AliasName="DESCR">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="PILIH"/>
        <DISPLAYWIDTH
          Value="20"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="app.fpp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="BudgetByClassLov"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="app.fpp.model.views.masterdata.ebs.BudgetByClassLov_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="srcText"
    Kind="viewcriteria"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="CLASS"/>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="setCategory"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery>
    <![CDATA[SELECT FcsViewItemMasterCategory.SET_CLASS, 
       FcsViewItemMasterCategory.SET_CLASS_DESC, 
       (FcsViewItemMasterCategory.SET_CLASS || ' ' || FcsViewItemMasterCategory.SET_CLASS_DESC) AS CLASS_SEARCH,
FcsViewItemMasterCategory.SET_CATEGORY
FROM APPS.FCS_VIEW_ITEM_MASTER_CATEGORY FcsViewItemMasterCategory
WHERE FcsViewItemMasterCategory.SET_CATEGORY = :setCategory
AND FcsViewItemMasterCategory.TYPE_TABLE = 'MASTER'
AND FcsViewItemMasterCategory.ITEM_ID IS NULL
GROUP BY FcsViewItemMasterCategory.SET_CLASS, FcsViewItemMasterCategory.SET_CLASS_DESC, FcsViewItemMasterCategory.SET_CATEGORY
ORDER BY SET_CLASS_DESC]]>
  </SQLQuery>

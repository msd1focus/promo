    IsPersistent="false"
    PrecisionRule="true"
    Precision="281"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CLASS_SEARCH"
    Expression="CLASS_SEARCH"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="281"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="CLASSIFICATION"/>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="SetCategory"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="40"

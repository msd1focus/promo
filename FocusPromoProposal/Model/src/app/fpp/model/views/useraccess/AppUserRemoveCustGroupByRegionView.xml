<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="AppUserRemoveCustGroupByRegionView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="false"
  RowClass="app.fpp.model.views.useraccess.AppUserRemoveCustGroupByRegionViewRowImpl"
  ComponentClass="app.fpp.model.views.useraccess.AppUserRemoveCustGroupByRegionViewImpl"
  PageIterMode="Full"
  UseGlueCode="false"
  SelectList="AppUserCustGroup.CUST_GROUP, 
       AppUserCustGroup.USER_NAME"
  FromList="APP_USER_CUST_GROUP AppUserCustGroup, APPS.AR_CUSTOMERS ArCustomers"
  Where="AppUserCustGroup.CUST_GROUP = ArCustomers.ATTRIBUTE1
AND ArCustomers.ATTRIBUTE3 NOT IN (SELECT AUR.REGION_CODE FROM APP_USER_REGION AUR WHERE AUR.USER_NAME = :userName)
AND AppUserCustGroup.USER_NAME = :userName
GROUP BY AppUserCustGroup.CUST_GROUP, AppUserCustGroup.USER_NAME">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="app.fpp.model.views.useraccess.AppUserRemoveCustGroupByRegionView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="userName"
    Kind="where"
    Type="java.lang.String"/>
  <EntityUsage
    Name="AppUserCustGroup"
    Entity="app.fpp.model.entities.useraccess.AppUserCustGroup"/>
  <EntityUsage
    Name="ArCustomers"
    Entity="app.fpp.model.entities.masterdata.ebs.ArCustomers"
    SourceUsage="app.fpp.model.views.useraccess.AppUserRemoveCustGroupByRegionView.AppUserCustGroup"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <ViewAttribute
    Name="CustGroup"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="CustGroup"
    EntityUsage="AppUserCustGroup"
    AliasName="CUST_GROUP"/>
  <ViewAttribute
    Name="UserName"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserName"
    EntityUsage="AppUserCustGroup"
    AliasName="USER_NAME"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="app.fpp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

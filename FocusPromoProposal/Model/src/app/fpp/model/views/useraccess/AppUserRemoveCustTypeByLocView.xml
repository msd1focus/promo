<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="AppUserRemoveCustTypeByLocView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  SelectList="AppUserCustType.CUST_TYPE, 
       AppUserCustType.USER_NAME"
  FromList="APP_USER_CUST_TYPE AppUserCustType, APPS.AR_CUSTOMERS ArCustomers"
  Where="AppUserCustType.CUST_TYPE = ArCustomers.ATTRIBUTE8
AND ArCustomers.ATTRIBUTE5 NOT IN (SELECT AUL.LOCATION_CODE FROM APP_USER_LOC AUL WHERE AUL.USER_NAME = :userName)
AND AppUserCustType.USER_NAME = :userName
GROUP BY AppUserCustType.CUST_TYPE, AppUserCustType.USER_NAME"
  BindingStyle="OracleName"
  CustomQuery="false"
  RowClass="app.fpp.model.views.useraccess.AppUserRemoveCustTypeByLocViewRowImpl"
  ComponentClass="app.fpp.model.views.useraccess.AppUserRemoveCustTypeByLocViewImpl"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="app.fpp.model.views.useraccess.AppUserRemoveCustTypeByLocView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="userName"
    Kind="where"
    Type="java.lang.String"/>
  <EntityUsage
    Name="AppUserCustType"
    Entity="app.fpp.model.entities.useraccess.AppUserCustType"/>
  <EntityUsage
    Name="ArCustomers"
    Entity="app.fpp.model.entities.masterdata.ebs.ArCustomers"
    SourceUsage="app.fpp.model.views.useraccess.AppUserRemoveCustTypeByLocView.AppUserCustType"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <ViewAttribute
    Name="CustType"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="CustType"
    EntityUsage="AppUserCustType"
    AliasName="CUST_TYPE"/>
  <ViewAttribute
    Name="UserName"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserName"
    EntityUsage="AppUserCustType"
    AliasName="USER_NAME"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="app.fpp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

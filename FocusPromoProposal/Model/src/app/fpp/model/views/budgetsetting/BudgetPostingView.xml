    Name="YearlyBudgetUsed"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="YEARLY_BUDGET_USED"
    SQLType="NUMERIC"
